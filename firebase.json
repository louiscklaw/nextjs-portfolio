{
  "hosting": [
    {
      "target": "staging",
      "public": "dist/public",
      "rewrites": [
        {
          "source": "**/**",
          "function": "staging_next"
        }
      ]
    },
    {
      "target":"production",
      "public": "dist/public",
      "rewrites": [
        {
          "source": "**/**",
          "function": "production_next"
        }
      ]
    }
  ],
  "functions": {
    "source": "dist/functions"
  }
}
